{
  "name": "cross-json",
  "version": "1.0.3",
  "description": "An npm module that reports property differences between JSON files. This can be useful to check whether your JSON-based internationalization files contain every translation key for every language, for instance.",
  "main": "dist/cross-json.js",
  "files": [
    "dist/*",
    "bin/*"
  ],
  "registry": "npm",
  "keywords": [
    "cross-json",
    "cross",
    "json",
    "comparison",
    "translation",
    "completion",
    "i18n",
    "properties",
    "property"
  ],
  "bin": {
    "cross-json": "./bin/cross-json.js"
  },
  "scripts": {
    "compile": "babel src -d dist",
    "prepublishOnly": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdex/cross-json.git"
  },
  "author": "kdex",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kdex/cross-json/issues"
  },
  "homepage": "https://github.com/kdex/cross-json#readme",
  "dependencies": {
    "chalk": "^2.4.0",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "babel-preset-minify": "^0.4.0"
  }
}
